Host pi.local
  HostName     192.168.1.64
  User         pi
  IdentityFile ~/.ssh/id_rsa.pi

Host github.com
  User         jjuarez
  IdentityFile ~/.ssh/id_rsa.clarity.github

Host gitlab.com
  User         jjuarez
  IdentityFile ~/.ssh/id_rsa.mundokids

Host github.ibm.com
  User         javier.juarez
  IdentityFile ~/.ssh/id_rsa.ibm

Host *
  UserKnownHostsFile    /dev/null
# IdentitiesOnly        yes
  StrictHostKeyChecking no
  VerifyHostKeyDNS      no
  TCPKeepAlive          yes
  ForwardX11            no
  ForwardAgent          no
  ServerAliveInterval   15
  ServerAliveCountMax   4
  Compression           yes
  CheckHostIP           no
  LogLevel              quiet
  Port                  22

