Host pi.local
  HostName     192.168.1.64
  User         pi
  IdentityFile ~/.ssh/id_ed25519.pi

Host github.com
  User jjuarez

Host gitlab.com
  User jjuarez

Host github.ibm.com
  User javier-juarez-martinez

##
## SSH jump host for QNet and OpenQ networks
##
Host saimaa
  Hostname saimaa.cloud9.ibm.com
  User runtimedeployusr
  IdentityFile ~/.ssh/id_rsa.ibm.runtimedeployusr.qnet

Host champlaincanal
  Hostname champlaincanal-nat.watson.ibm.com
  User runtimedeployusr
  IdentityFile ~/.ssh/id_rsa.ibm.runtimedeployusr.openq

Host ibmq-bastion.cloud9.ibm.com
  HostName ibmq-bastion.cloud9.ibm.com

Host *.ccf.local
  ProxyJump ibmq-bastion

Host *
  AddKeysToAgent        yes
  UserKnownHostsFile    /dev/null
  StrictHostKeyChecking no
  VerifyHostKeyDNS      no
  TCPKeepAlive          yes
  ForwardX11            no
  ForwardAgent          yes
  ServerAliveInterval   15
  ServerAliveCountMax   4
  Compression           yes
  CheckHostIP           no
  LogLevel              INFO
  Port                  22
  SetEnv                LC_ALL=en_US.UTF8
