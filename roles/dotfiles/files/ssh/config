##
## Global options
##
Host *
  UserKnownHostsFile       ~/Developer/ibm/q/projects/infra/ops-datacenter/ansible_known_hosts
  StrictHostKeyChecking    no
  VerifyHostKeyDNS         no
# PreferredAuthentications publickey
  GSSAPIAuthentication     yes
  ForwardX11               no
  AddKeysToAgent           yes
  ForwardAgent             yes
  TCPKeepAlive             yes
  ServerAliveInterval      15
  ServerAliveCountMax      4
  Compression              yes
  ControlMaster            auto
  ControlPath              /tmp/ssh_mux_%h_%p_%r.sock
  ControlPersist           86400s
  CheckHostIP              no
  Port                     22
  SetEnv                   LC_ALL=en_US.UTF8
  LogLevel                 INFO
