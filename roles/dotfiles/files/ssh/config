Host *
  AddKeysToAgent        yes
  UserKnownHostsFile    /dev/null
  StrictHostKeyChecking no
  VerifyHostKeyDNS      no
  TCPKeepAlive          yes
  ForwardX11            no
  ForwardAgent          yes
  ServerAliveInterval   15
  ServerAliveCountMax   4
  Compression           yes
  CheckHostIP           no
  LogLevel              quiet
  Port                  22
  SetEnv                LC_ALL=en_US.UTF8
