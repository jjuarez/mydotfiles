Host *
# ProxyCommand          ssh -q gen01 nc -q0 %h %p
  UserKnownHostsFile    /dev/null
  ControlMaster         auto
  ControlPath           ~/.ssh/ssh_control_%h_%p_%r
  ControlPersist        1h
  IdentityFile          ~/.ssh/id_dsa
  KeepAlive             yes
  ForwardX11            no
  ForwardAgent          yes
  ServerAliveInterval   15
  ServerAliveCountMax   4
  Compression           yes
  CheckHostIP           yes
  VerifyHostKeyDNS      no
  StrictHostKeyChecking no
  User                  root
  Port                  22

