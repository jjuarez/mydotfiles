Host raspberrypi
  HostName 192.168.1.2
  IdentityFile ~/.ssh/id_dsa.raspberrypi
  User root

Host github.com
  HostName github.com
  IdentityFile ~/.ssh/id_dsa.tuenti
  User jjuarez

Host dbbackup1
  HostName dbbackup1.tuenti.int
  IdentitiesOnly yes

Host dev?
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_dsa.tuenti
  User jjuarez

Host gen01.tuenti.int
  HostName gen01.tuenti.int
  IdentitiesOnly yes
  User jjuarez

Host bastion
  HostName gen01.tuenti.int
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_dsa.tuenti
  User root

Host *
  ControlMaster         auto
  ControlPath           ~/.ssh/%r@%h:%p
  UserKnownHostsFile    /dev/null
  IdentityFile          ~/.ssh/id_dsa
  KeepAlive             yes
  ForwardX11            no
  ForwardAgent          yes
  ServerAliveInterval   15
  ServerAliveCountMax   4
  Compression           yes
  CheckHostIP           yes
  VerifyHostKeyDNS      no
  StrictHostKeyChecking no
  User                  root
  Port                  22

