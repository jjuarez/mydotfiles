##
# Spinnaker PoC
Host spinnaker-start
  HostName      ec2-54-144-238-249.compute-1.amazonaws.com
  IdentityFile  ~/.ssh/Amazon.4iq.Systems.pem
  ControlMaster yes
  ControlPath   ~/.ssh/spinnaker-tunnel.ctl
  LocalForward  9000 127.0.0.1:9000
  LocalForward  8084 127.0.0.1:8084
  LocalForward  8087 127.0.0.1:8087
  RequestTTY    no
  User          ubuntu

Host spinnaker-stop
  HostName      ec2-54-144-238-249.compute-1.amazonaws.com
  IdentityFile  ~/.ssh/Amazon.4iq.Systems.pem
  ControlMaster yes
  ControlPath   ~/.ssh/spinnaker-tunnel.ctl
  RequestTTY    no
  User          ubuntu

Host vpn-pro-1.mad.4iq.int
  HostName 192.168.0.155

Host *
  IdentitiesOnly         yes
  IdentityFile           ~/.ssh/id_rsa
  UserKnownHostsFile     /dev/null
  StrictHostKeyChecking  no
  VerifyHostKeyDNS       no
  KeepAlive              yes
  ForwardX11             no
  ForwardAgent           yes
  ServerAliveInterval    15
  ServerAliveCountMax    4
  Compression            yes
  CheckHostIP            yes
  LogLevel               quiet
  User                   root
  Port                   22

