Host github.com
  HostName     github.com
  User         jjuarez
  IdentityFile ~/.ssh/id_rsa.clarity.github

Host gitlab.clarity.ai
  HostName     gitlab.clarity.ai
  User         javier.juarez
  IdentityFile ~/.ssh/id_rsa.clarity.gitlab

Host default
  HostName 127.0.0.1
  User vagrant
  Port 2222
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile ~/workspace/clarity/devops/PROJECT/.vagrant/machines/default/virtualbox/private_key
  IdentitiesOnly yes
  LogLevel FATAL

Host *
  User                   ubuntu
  UserKnownHostsFile     /dev/null
  StrictHostKeyChecking  no
  VerifyHostKeyDNS       no
  TCPKeepAlive           yes
  ForwardX11             no
  ForwardAgent           yes
  ServerAliveInterval    15
  ServerAliveCountMax    4
  Compression            yes
  CheckHostIP            yes
  LogLevel               quiet
  Port                   22
