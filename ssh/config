Host github.com
  HostName     github.com
  IdentityFile ~/.ssh/id_rsa.github.clarity
  User         jjuarez

Host gitlab.com
  Hostname     gitlab.com
  IdentityFile ~/.ssh/id_rsa.gitlab.com
  User         javier.juarez@fon.com

Host bastion
  HostName bastion-fon-mgmt-pro-001.mgmt.homewifi.fon.com
  User     admin

Host default
  HostName 127.0.0.1
  User vagrant
  Port 2222
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile ~/workspace/fon/devops/misc/mqtt_custom_plugin/.vagrant/machines/default/virtualbox/private_key
  IdentitiesOnly yes
  LogLevel FATAL

Host *
  UserKnownHostsFile     /dev/null
  StrictHostKeyChecking  no
  VerifyHostKeyDNS       no
  TCPKeepAlive           yes
  ForwardX11             no
  ForwardAgent           yes
  ServerAliveInterval    15
  ServerAliveCountMax    4
  Compression            yes
  CheckHostIP            yes
  LogLevel               quiet
  Port                   22
