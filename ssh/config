Host github.com
  HostName     github.com
  IdentityFile ~/.ssh/id_dsa.tuenti
  User         jjuarez

Host raspberrypi
  HostName     192.168.0.3
  IdentityFile ~/.ssh/id_dsa.raspberrypi
  User         root

Host dev1 dev2 dev3 dev4 dev5 dev6
  HostName     %h.tuenti.int
  IdentityFile ~/.ssh/id_dsa.tuenti
  User         jjuarez

Host hg3 hg4
  Hostname    %h.tuenti.int
  IdentityFile ~/.ssh/id_dsa.tuenti


Host *
# ControlMaster          auto
# ControlPath            /tmp/ssh_mux_%h_%p_%r
  IdentitiesOnly         yes
  PubkeyAcceptedKeyTypes +ssh-dss
  IdentityFile           ~/.ssh/id_dsa
  UserKnownHostsFile     /dev/null
  StrictHostKeyChecking  no
  VerifyHostKeyDNS       no
  KeepAlive              yes
  ForwardX11             no
  ForwardAgent           yes
  ServerAliveInterval    15
  ServerAliveCountMax    4
  Compression            yes
  CheckHostIP            yes
  LogLevel               quiet
  User                   root
  Port                   22

