# Office
Host puppet.4iq.office
  HostName puppet.4iq.office
  User     vagrant

Host *
  IdentitiesOnly         yes
  IdentityFile           ~/.ssh/id_rsa
  UserKnownHostsFile     /dev/null
  StrictHostKeyChecking  no
  VerifyHostKeyDNS       no
  KeepAlive              yes
  ForwardX11             no
  ForwardAgent           yes
  ServerAliveInterval    15
  ServerAliveCountMax    4
  Compression            yes
  CheckHostIP            yes
  LogLevel               quiet
  User                   root
  Port                   22

