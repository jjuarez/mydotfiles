[user]
	name  = Javier Juarez
	email = javier.juarez@tuenti.com
[color]
  status = auto
  branch = auto
[alias]
  co      = checkout
  ci      = commit
  br      = branch
  st      = status
  last    = log -1 HEAD
  unstage = reset HEAD --
	lg      = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[github]
  user = jjuarez
[push]
  default = matching
[core]
	excludesfile = /Users/jjuarez/.gitignore_global
